// This is a generic overview. Differences exist in layout for plaintext and JSON

common-name: "Common name" (string)
key: "XXXGPYYYY" (string, ISO 3166-1 alpha-3 + GP + Year)
name: "XXX" (string, offical race name)
date: YYYY-MM-DD (date)
city: "City" (string)
country: "Country" (string)
iso: "XX" (string, ISO 3166-1 alpha-2)
race-time: XX:XX (local time)
utc-offset: +/- XX:XX (time zone)

circuit: "Circuit" (string)
drs-zones: N (integer)
laps: N (integer)
lap-distance: N.nnn (double, km)
race-distance: NNN.nnn (double, km)
lap-record: X:xx.xxx (duration)
lap-record-holder: "Name" (string)
lap-record-year: YYYY (year)

campaign: YYYY (year)
fastest-lap-holder: "Name" (string)
fastest-lap-time: X:xx.xxx (duration)
fastest-lap-number: N (integer)
results: [PX: name] (array, Position:Name)
penalties: [name, code, on-lap, reason] (array, Driver:CODE:Lap number:Reason)
